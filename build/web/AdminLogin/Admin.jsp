            // Display SweetAlert with the success message
            Swal.fire({
                icon: 'success',
                title: 'SuccessFul!',
                text: successMessage,
                confirmButtonText: 'OK'
            });

            // Clear the "success" attribute from the session to avoid showing the alert again
            <% session.removeAttribute("success"); %>
        }
